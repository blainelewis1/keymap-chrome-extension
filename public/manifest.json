{
  "name": "KeyMap",
  "version": "0.1",
  "description": "Learn keyboard shortcuts to improve your productivity.",
  "manifest_version": 2,
  "homepage_url": "http://blainelewis.ca/KeyMap",
  "content_scripts": [
    {
      "matches": [
        "*://www.overleaf.com/*",
        "*://app.slack.com/*"
      ],
      "css": [
        "/static/css/main.css"
      ],
      "js": [
        "/static/js/main.js"
      ]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "iconalpha.png"
  }
}